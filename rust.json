{
  "packageRules": [
    {
      "matchManagers": ["cargo"],
      "matchPackagePatterns": [
        ".*-distro",
        "fluence-.*",
        "avm-server",
        "air-interpreter-wasm",
        "ccp-.*",
        "marine-rs-sdk",
        "marine-rs-sdk-test"
      ],
      "semanticCommitScope": "deps",
      "enabled": true
    },
    {
      "matchManagers": ["cargo"],
      "matchPackagePatterns": [
        "avm-server",
        "air-interpreter-wasm"
      ],
      "groupName": "avm"
    },
    {
      "matchManagers": ["cargo"],
      "matchPackagePatterns": ["fluence-spell-dtos", "fluence-spell-distro"],
      "groupName": "fluence-spell"
    },
    {
      "matchManagers": ["cargo"],
      "matchPackagePatterns": ["marine-rs-sdk", "marine-rs-sdk-test"],
      "groupName": "marine-rs-sdk"
    },
    {
      "matchManagers": ["cargo"],
      "matchPackagePatterns": ["^libp2p.*", "prometheus-client", "multihash"],
      "groupName": "libp2p"
    },
    {
      "matchManagers": ["cargo"],
      "matchPackagePatterns": ["ccp-rpc-client", "ccp-shared", "cpu-utils"],
      "groupName": "ccp"
    },
    {
      "matchManagers": ["cargo"],
      "matchPackagePatterns": ["opentelemetry", "opentelemetry-otlp", "tracing-opentelemetry", "opentelemetry_sdk"],
      "groupName": "opentelemetry"
    }
  ]
}
